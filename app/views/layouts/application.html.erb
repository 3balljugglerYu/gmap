<!DOCTYPE html>
<html>
  <head>
    <title>Gmap</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['GCP_KEY_ID'] %>&callback=initMap" defer></script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
